<p id="notice"><%= notice %></p>

<h1>Items In Carts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @items_in_carts.each do |items_in_cart| %>
      <tr>
        <td><%= link_to 'Show', items_in_cart %></td>
        <td><%= link_to 'Edit', edit_items_in_cart_path(items_in_cart) %></td>
        <td><%= link_to 'Destroy', items_in_cart, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Items In Cart', new_items_in_cart_path %>
